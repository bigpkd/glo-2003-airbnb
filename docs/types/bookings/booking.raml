#%RAML 1.0 DataType
type: object
displayName: Booking
uses:
  types: ../../libraries/types.raml
properties:
  arrivalDate:
      type: date-only
  numberOfNights:
      type: types.NumberOfNights
  package:
      type: types.PackageName
  colonySize:
      description: Colony size for booking, only displayed if bed has a cohabitation lodging mode
      required: false
      type: types.ColonySize
  total:
      description: Total for booking
      type: types.Price
